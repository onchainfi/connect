{
  "name": "@onchainfi/connect",
  "version": "2.0.0",
  "description": "Complete wallet authentication and x402 payment SDK with multi-chain support (EVM + Solana), pre-built UI components, and zero-config setup",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "web3",
    "wallet",
    "authentication",
    "privy",
    "x402",
    "payments",
    "crypto",
    "react"
  ],
  "author": "0xvsr",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/onchainfi/connect"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0"
  },
  "dependencies": {
    "@privy-io/react-auth": "^3.4.1",
    "@privy-io/wagmi": "^2.0.2",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.90.5",
    "lucide-react": "^0.548.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "packageManager": "pnpm@9.0.0",
  "publishConfig": {
    "access": "public"
  }
}
